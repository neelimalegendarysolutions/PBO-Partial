            <label>Mapping found</label>
        </rules>
    </decisions>
    <description>Use Domain to Queue mapping from the Custom Metadata</description>
    <environments>Default</environments>
    <formulas>
        <description>Extract Email Domain from the Case Web Email field</description>
        <name>EmailDomain</name>
        <dataType>String</dataType>
        <expression>RIGHT(
{!input_record.SuppliedEmail}, 
LEN({!input_record.SuppliedEmail})  - FIND(&apos;@&apos;, {!input_record.SuppliedEmail})
)</expression>
    </formulas>
