        <name>is_Queue_Name_found</name>
        <label>is Queue Name found?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Get_generic_Queue</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not Found</defaultConnectorLabel>
        <rules>
            <name>Mapping_found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
